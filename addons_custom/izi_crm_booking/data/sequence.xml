<odoo>
    <data noupdate="1">
        <record forcecreate="1" id="ev_service_booking_name_seq_id" model="ir.sequence">
            <field name="name">EV_SERVICE_BOOKING_NAME_SEQ</field>
            <field name="code">ev_service_booking_name_seq</field>
            <field name="padding" eval="4"/>
            <field name="prefix">BOOKING%(y)s%(month)s/</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>


        <record forcecreate="1" id="ev_service_meeting_name_seq_id" model="ir.sequence">
            <field name="name">EV_SERVICE_MEETING_NAME_SEQ</field>
            <field name="code">ev_service_meeting_name_seq</field>
            <field name="padding" eval="4"/>
            <field name="prefix">MEETING%(y)s%(month)s/</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>