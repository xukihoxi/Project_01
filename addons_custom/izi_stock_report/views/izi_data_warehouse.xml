        <record id="izi_data_warehouse_day_form" model="ir.ui.view">
            <field name="name">izi.data.warehouse.day</field>
            <field name="model">izi.data.warehouse.day</field>
            <field name="arch" type="xml">
                <form string="Data warehouse">
                    <group>
                        <group>
                            <field name="warehouse_id"/>
                            <field name="location_id"/>
                            <field name="product_id"/>
                            <field name="lot_id"/>
